<?xml version="1.0"?>
<security xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="restriction.xsd">
    <constraint>
        <url-pattern>/admin/*</url-pattern>
        <role>admin</role >
    </constraint>
    <constraint>
        <url-pattern>/user/*</url-pattern>
        <role>user</role >
    </constraint>
    <constraint>
        <url-pattern>/order</url-pattern>
        <role>user</role >
    </constraint>
</security>
